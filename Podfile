project 'UniversalAnsteuerung.xcodeproj'

# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

target 'UniversalAnsteuerung' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'CocoaMQTT' 

  # Pods for UniversalAnsteuerung

  target 'UniversalAnsteuerungTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'CocoaMQTT'
  end

  target 'UniversalAnsteuerungUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'CocoaMQTT'
  end

end
